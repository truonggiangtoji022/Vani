function clearTooltip(e){e.currentTarget.setAttribute('class','btn coupon-code-button');e.currentTarget.removeAttribute('aria-label');}
function showTooltip(elem,msg){elem.setAttribute('class','btn coupon-code-button tooltipped tooltipped-s');elem.setAttribute('aria-label',msg);}
function fallbackMessage(action){let actionMsg='';let actionKey=(action==='cut'?'X':'C');if(/iPhone|iPad/i.test(navigator.userAgent)){actionMsg='No support :(';}else if(/Mac/i.test(navigator.userAgent)){actionMsg='Press âŒ˜-'+actionKey+' to '+action;}else{actionMsg='Press Ctrl-'+actionKey+' to '+action;}
return actionMsg;}